{
  "name": "@vikaspogu/rhdh-datetime",
  "version": "0.0.3",
  "description": "DeveloperHub dynamic plugin for datetime",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "backstage": {
    "role": "backend-plugin-module"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@backstage/cli": "0.26.2",
    "@janus-idp/cli": "1.7.10"
  },
  "scripts": {
    "clean": "backstage-cli package clean",
    "build": "backstage-cli package build",
    "clean-dynamic-sources": "yarn clean && rm -Rf node_modules",
    "export-dynamic": "janus-cli package export-dynamic-plugin --override-interop default"
  },
  "files": [
    "dist",
    "dist-dynamic/*.*",
    "dist-dynamic/dist/**"
  ],
  "dependencies": {
    "@backstage/backend-dynamic-feature-service": "^0.2.8",
    "@backstage/backend-plugin-api": "^0.6.16",
    "@backstage/plugin-scaffolder-node": "^0.4.2"
  }
}
