{
  "name": "@vikaspogu/rhdh-timesaver-backend",
  "version": "0.0.13",
  "description": "DeveloperHub dynamic plugin for otimesaver-backend",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "backstage": {
    "role": "backend-plugin"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@backstage/cli": "0.25.2",
    "@janus-idp/cli": "1.7.5"
  },
  "scripts": {
    "clean": "backstage-cli package clean",
    "build": "backstage-cli package build",
    "clean-dynamic-sources": "yarn clean && rm -Rf node_modules dist-types dist-dynamic",
    "export-dynamic": "janus-cli package export-dynamic-plugin --embed-package @tduniec/backstage-plugin-time-saver-backend --override-interop default"
  },
  "files": [
    "dist",
    "dist-dynamic/*.*",
    "dist-dynamic/dist/**"
  ],
  "dependencies": {
    "@backstage/backend-dynamic-feature-service": "0.2.3",
    "@backstage/backend-plugin-api": "0.6.13",
    "@tduniec/backstage-plugin-time-saver-backend": "^0.1.10"
  }
}
