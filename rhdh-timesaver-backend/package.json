{
  "name": "@vikaspogu/rhdh-timesaver-backend",
  "version": "0.0.8",
  "description": "DeveloperHub dynamic plugin for otimesaver-backend",
  "main": "src/index.ts",
  "license": "Apache-2.0",
  "backstage": {
    "role": "backend-plugin"
  },
  "devDependencies": {
    "@backstage/cli": "0.26.2",
    "@janus-idp/cli": "1.7.10"
  },
  "scripts": {
    "clean": "backstage-cli package clean",
    "build": "backstage-cli package build",
    "clean-dynamic-sources": "yarn clean && rm -Rf node_modules dist-types dist-dynamic",
    "export-dynamic": "janus-cli package export-dynamic-plugin --embed-package @tduniec/backstage-plugin-time-saver-backend"
  },
  "files": [
    "dist",
    "dist-dynamic/*.*",
    "dist-dynamic/dist/**"
  ],
  "dependencies": {
    "@backstage/backend-dynamic-feature-service": "^0.2.8",
    "@tduniec/backstage-plugin-time-saver-backend": "^0.1.10"
  }
}
