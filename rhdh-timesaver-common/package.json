{
  "name": "@vikaspogu/rhdh-timesaver-common",
  "version": "0.0.2",
  "description": "DeveloperHub dynamic plugin for timesaver-common",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "backstage": {
    "role": "frontend-plugin"
  },
  "devDependencies": {
    "@backstage/cli": "0.26.2",
    "@janus-idp/cli": "1.7.10"
  },
  "scripts": {
    "clean": "backstage-cli package clean",
    "build": "backstage-cli package build",
    "clean-dynamic-sources": "yarn clean && rm -Rf node_modules dist-types dist-dynamic",
    "export-dynamic": "janus-cli package export-dynamic-plugin --embed-as-dependencies @tduniec/backstage-plugin-time-saver-common --embed-package @tduniec/backstage-plugin-time-saver-common"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@tduniec/backstage-plugin-time-saver-common": "^0.1.2"
  }
}
