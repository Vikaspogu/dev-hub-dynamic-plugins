{
  "name": "@vikaspogu/rhdh-http-request",
  "version": "0.0.1",
  "description": "DeveloperHub dynamic plugin for open-dora",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "backstage": {
    "role": "backend-plugin-module"
  },
  "devDependencies": {
    "@backstage/cli": "0.26.2",
    "@janus-idp/cli": "1.7.10",
    "@backstage/backend-common": "0.21.6"
  },
  "scripts": {
    "clean": "backstage-cli package clean",
    "build": "backstage-cli package build",
    "export-dynamic": "janus-cli package export-dynamic-plugin"
  },
  "files": [
    "dist",
    "dist-scalprum",
    "dist-dynamic/*.*",
    "dist-dynamic/dist/**",
    "dist-dynamic/alpha/*"
  ],
  "scalprum": {
    "name": "vikaspogu.rhdh-http-request",
    "exposedModules": {
      "PluginRoot": "./src/index.ts"
    }
  },
  "dependencies": {
    "@roadiehq/scaffolder-backend-module-http-request": "^4.1.10"
  }
}
